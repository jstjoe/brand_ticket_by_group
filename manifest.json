{
  "name": "Brand Ticket Email",
  "author": {
    "name": "Zendesk Services",
    "email": "services@zendesk.com"
  },

  "defaultLocale": "en",
  "private": true,
  "location": ["new_ticket_sidebar", "ticket_sidebar"],
  "version": "0.1.3",
  "frameworkVersion": "1.0",
  "noTemplate": true,
  "parameters": [
    {
      "name": "brand_field_id",
      "type": "number",
      "required": true,
      "default": 23823218
    },
    {
      "name": "mapping",
      "type": "text",
      "required": true
    },
    {
      "name": "force_selection_of_brand",
      "type": "checkbox"
    },
    {
      "name": "mandatory_requester_email",
      "type": "checkbox",
      "default": false
    },
    {
      "name":"new_tickets?",
      "type":"checkbox",
      "default":true
    },
    {
      "name":"existing_tickets?",
      "type":"checkbox",
      "default":true
    }
  ]
}
